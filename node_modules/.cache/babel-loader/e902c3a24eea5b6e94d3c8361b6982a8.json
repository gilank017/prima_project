der\"\n                  item-text=\"name\"\n                  item-value=\"id\"\n                ></v-select>\n              </v-col>\n              <v-col cols=\"4\"></v-col>\n              <v-col cols=\"5\" md=\"3\">\n                <v-select\n                  label=\"Currency\"\n                  v-model=\"inputData.currency\"\n                  :items=\"currencyList\"\n                  @change=\"changeCurrency\"\n                ></v-select>\n              </v-col>\n              <v-col cols=\"5\">\n                <v-text-field\n                  label=\"Customer Name\"\n                  v-model=\"inputData.name_client\"\n                  :items=\"dataCustomer\"\n                  item-text=\"name\"\n                  item-value=\"name\"\n                >\n                </v-text-field>\n              </v-col>\n              <v-col cols=\"2\"></v-col>\n              <v-col cols=\"5\" md=\"3\">\n                <v-menu>\n                  <template v-slot:activator=\"{ on, attrs }\">\n                    <v-text-field\n                      label=\"Order Date\"\n                      append-icon=\"mdi-calendar\"\n                      readonly\n                      v-bind=\"attrs\"\n                      v-on=\"on\"\n                      :value=\"inputData.date\"\n                    ></v-text-field>\n                  </template>\n                  <v-date-picker v-model=\"inputData.date\" landscape>\n                  </v-date-picker>\n                </v-menu>\n              </v-col>\n              <v-col cols=\"3\">\n                <v-select\n                  label=\"Status\"\n                  v-model=\"inputData.status\"\n                  :items=\"statusList\"\n                  item-text=\"name\"\n                  item-value=\"id\"\n                ></v-select>\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-form>\n      </v-col>\n      <v-col cols=\"12\">\n        <v-card-title>Create Orders</v-card-title>\n        <v-form>\n          <v-container>\n            <v-row>\n              <v-col cols=\"8\">\n                <v-list-title><h3>Detail Product</h3></v-list-title>\n              </v-col>\n              <v-col cols=\"4\">\n                <v-dialog v-model=\"dialog\" max-width=\"700px\">\n                  <template v-slot:activator=\"{ on, attrs }\">\n                    <v-btn class=\"ma-2\" color=\"primary\" v-on=\"on\" v-bind=\"attrs\"\n                      ><v-icon>mdi-plus</v-icon>Add Product</v-btn\n                    >\n                  </template>\n                  <v-card tile>\n                    <v-card-title>\n                      <span class=\"headline\">Add Product</span>\n                    </v-card-title>\n                    <v-card-text>\n                      <v-container>\n                        <v-form>\n                          <v-row>\n                            <v-col cols=\"12\" md=\"8\">\n                              <v-autocomplete\n                                v-model=\"selectProduct\"\n                                label=\"Search Product\"\n                                :items=\"dataProduct\"\n                                :search-input.sync=\"searchInput\"\n                                item-text=\"product_name\"\n                                item-value=\"product_name\"\n                                return-object\n                              >\n                                <v-btn\n                                  small\n                                  slot=\"append-outer\"\n                                  @click=\"addProduct\"\n                                  color=\"primary\"\n                                >\n                                  Add Product\n                                </v-btn>\n                                <template v-slot:item=\"data\">\n                                  <template\n                                    v-if=\"typeof data.item !== 'object'\"\n                                  >\n                                    <v-list-item-content\n                                      v-text=\"data.item\"\n                                    ></v-list-item-content>\n                                  </template>\n                                  <template v-else>\n                                    <v-list-item-image>\n                                      <v-img\n                                        class=\"ma-2 pa-2\"\n                                        :src=\"data.item.image_url\"\n                                        max-width=\"40\"\n                                        max-height=\"40\"\n                                      >\n                                      </v-img>\n                                    </v-list-item-image>\n                                    <v-list-item-content>\n                                      <v-list-item-title>{{\n                                        data.item.product_name\n                                      }}</v-list-item-title>\n                                      <v-list-item-sub-title\n                                        >{{ data.item.price }}\n                                        {{\n                                          data.item.currency\n                                        }}</v-list-item-sub-title\n                                      >\n                                    </v-list-item-content>\n                                  </template>\n                                </template>\n                              </v-autocomplete>\n                            </v-col>\n                            <v-col cols=\"12\">\n                              <v-list>\n                                <v-list-title\n                                  ><h3>Selected Product</h3></v-list-title\n                                >\n                                <v-list-item\n                                  v-for=\"(value, index) in valueProduct\"\n                                  :key=\"index\"\n                                >\n                                  <v-list-item-image>\n                                    <v-img\n                                      class=\"ma-2 pa-2\"\n                                      :src=\"value.image_url\"\n                                      max-width=\"40\"\n                                      max-height=\"40\"\n                                    >\n                                    </v-img>\n                                  </v-list-item-image>\n                                  <v-list-item-title\n                                    >Product:\n                                    {{ value.product_name }}\n                                    {{ value.product_title }}\n                                  </v-list-item-title>\n                                  <v-list-item-subtitle>\n                                    Price : {{ value.price }}\n                                    {{ value.currency }}\n                                  </v-list-item-subtitle>\n                                  <v-list-item-action>\n                                    <v-icon\n                                      slot=\"append\"\n                                      color=\"red\"\n                                      @click=\"removeProduct\"\n                                    >\n                                      mdi-minus-circle-outline\n                                    </v-icon>\n                                  </v-list-item-action>\n                                </v-list-item>\n                              </v-list>\n                            </v-col>\n                          </v-row>\n                        </v-form>\n                      </v-container>\n                    </v-card-text>\n                    <v-divider class=\"mt-5\"></v-divider>\n                    <v-card-actions>\n                      <v-spacer></v-spacer>\n                      <v-btn color=\"error\" @click=\"close\"> Cancel </v-btn>\n                      <v-btn color=\"primary\" @click=\"save\"> Save </v-btn>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n                <v-dialog v-model=\"dialogNote\" max-width=\"600px\" scrollable>\n                  <v-card tile>\n                    <v-card-title>\n                      <span class=\"headline\">Add Note</span>\n                    </v-card-title>\n                    <v-card-text>\n                      <v-container>\n                        <v-form>\n                          <v-row>\n                            <v-textarea\n                              v-model=\"noteProduct\"\n                              placeholder=\"Add Some Note. . .\"\n                            ></v-textarea>\n                          </v-row>\n                        </v-form>\n                      </v-container>\n                    </v-card-text>\n                    <v-divider class=\"mt-5\"></v-divider>\n                    <v-card-actions>\n                      <v-spacer></v-spacer>\n                      <v-btn color=\"error\" @click=\"close\"> Cancel </v-btn>\n                      <v-btn color=\"primary\" @click=\"saveNote\"> Save </v-btn>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-simple-table>\n                  <template v-slot:default>\n                    <thead>\n                      <tr>\n                        <th class=\"text-left\">image</th>\n                        <th class=\"text-left\">Product Name</th>\n                        <th class=\"text-left\">Price</th>\n                        <th class=\"text-left\">Quantity</th>\n                        <th class=\"text-left\">Total</th>\n                        <th class=\"text-left\">Actions</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      <tr v-for=\"(value, index) in detailProduct\" :key=\"index\">\n                        <td>\n                          <v-img\n                            class=\"ma-2 pa-2\"\n                            :src=\"value.image_url\"\n                            max-width=\"70\"\n                            max-height=\"70\"\n                          ></v-img>\n                        </td>\n                        <td>\n                          <v-list>\n                            <v-list-item-title>\n                              <h3>\n                                {{ value.product_title }}\n                                {{ value.product_name }}\n                              </h3>\n                              <caption v-if=\"value.supplier_name != null\">\n                                By\n                                {{\n                                  value.supplier_name\n                                }}\n                              </caption>\n                            </v-list-item-title>\n                            <v-list-item-subtitle v-if=\"value.note != null\">\n                              Note : {{ value.note }}\n                            </v-list-item-subtitle>\n                            <v-list-item-subtitle>\n                              <v-btn\n                                text\n                                x-small\n                                @click=\"openNote(index)\"\n                                color=\"primary\"\n                                >Add Note</v-btn\n                              >\n                            </v-list-item-subtitle>\n                          </v-list>\n                        </td>\n                        <td>{{ value.price }}</td>\n                        <td>\n                          <v-text-field\n                            v-model=\"value.quantity\"\n                            class=\"editWidth\"\n                            type=\"number\"\n                            @change=\"calculateTotal(index)\"\n                          ></v-text-field>\n                        </td>\n\n                        <td>{{ value.total_price }}</td>\n                        <td>\n                          <v-icon\n                            v-model=\"value.remove\"\n                            @click=\"removeDetailProduct\"\n                            >clear</v-icon\n                          >\n                        </td>\n                      </tr>\n                    </tbody>\n                  </template>\n                </v-simple-table>\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-form>\n      </v-col>\n      <v-divider class=\"mt-5\"></v-divider>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"error\" @click=\"cancelData\"> Cancel </v-btn>\n        <v-btn color=\"primary\" @click=\"saveData\"> Save </v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-app>\n</template>\n\n<script>\nimport Proxy from \"@/services/proxy\";\nexport default {\n  name: \"editOrder\",\n  data() {\n    return {\n      date: \"\",\n      menu: false,\n      dialog: false,\n      dialogNote: false,\n      dataCustomer: [],\n      typeOrder: [\n        { id: 1, name: \"Income\" },\n        { id: 2, name: \"Outcome\" },\n      ],\n      inputData: {\n        name_client: \"\",\n        type: \"\",\n        currency: \"\",\n        date: \"\",\n        status: \"\",\n      },\n      snackbar: {\n        show: false,\n        text: \"Data Added Successfully\",\n        color: \"success\",\n      },\n      currencyList: [\"USD\", \"IDR\", \"CNY\"],\n      statusList: [\n        { id: 1, name: \"Pending\" },\n        { id: 2, name: \"Delivered\" },\n      ],\n      detailProduct: [],\n      detailProductList: {\n        id: \"\",\n        image_url: \"\",\n        note: \"\",\n        order_id: \"\",\n        price: \"\",\n        product_title: \"\",\n        quantity: \"\",\n        supplier_name: \"\",\n        total_price: \"\",\n        updated_at: \"\",\n      },\n      noteProduct: \"\",\n      selectProduct: {},\n      queryTerm: \"\",\n      valueProduct: {},\n      activeProduct: \"\",\n      productIndex: 0,\n      quantity: \"\",\n      totalProduct: 0,\n    };\n  },\n  watch: {\n    search: {\n      get() {\n        this.queryTerm;\n      },\n      set(searchInput) {\n        if (this.queryTerm !== searchInput) {\n          this.queryTerm = searchInput;\n          this.loadProduct();\n        }\n      },\n    },\n  },\n  created() {\n    this.loadProduct();\n  },\n\n  methods: {\n    changeCurrency() {\n      this.loadProduct();\n    },\n\n    loadProduct() {\n      const uri = \"https://api-dev.phantasmode.com\";\n      // get token from localStorage\n      const token = localStorage.getItem(\"access_token\");\n      // set the headers\n      const headers = {\n        Authorization: \"Bearer \" + token,\n        Accept: \"application/json\",\n      };\n\n      Proxy.get(\n        uri +\n          \"/api/products/\" +\n          \"?product_name=\" +\n          (this.queryTerm || \"\") +\n          \"&convertTo=\" +\n          this.inputData.currency,\n        { headers }\n      )\n        .then((res) => {\n          this.dataProduct = res.data.data;\n          this.queryTerm = res.data.total;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n\n    addProduct() {\n      const incomingDataProduct = this.selectProduct;\n      this.selectProduct = {};\n      this.valueProduct.push(incomingDataProduct);\n    },\n    close() {\n      this.dialog = false;\n      this.dialogNote = false;\n      this.selectProduct = null;\n    },\n    save() {\n      this.detailProduct = this.valueProduct;\n      console.log(this.detailProduct);\n      this.close();\n    },\n    removeProduct(index) {\n      this.valueProduct.splice(index, 1);\n    },\n    removeDetailProduct(index) {\n      this.detailProduct.splice(index, 1);\n      this.detailProduct[this.productIndex].remove = true;\n    },\n    openNote(indexProduct) {\n      this.activeProduct = indexProduct;\n      this.dialogNote = true;\n    },\n    saveNote() {\n      this.detailProduct[this.activeProduct].note = this.noteProduct;\n      this.noteProduct = \"\";\n      this.dialogNote = false;\n    },\n    calculateTotal() {\n      var total = this.detailProduct.price * this.detailProduct.quantity;\n      this.totalProduct = total;\n      console.log(total);\n    },\n    cancelData() {\n      this.$router.push(\"/Menu/Order/\");\n    },\n    saveData() {\n      const uri = \"https://api-dev.phantasmode.com\";\n      // get token from localStorage\n      const token = localStorage.getItem(\"access_token\");\n      // set the headers\n      const headers = {\n        Authorization: \"Bearer \" + token,\n        Accept: \"application/json\",\n      };\n      const body = {\n        type: this.inputData.type,\n        client_name: this.inputData.name_client,\n        currency: this.inputData.currency,\n        order_date: this.inputData.date,\n        status: this.inputData.status,\n        items: [\n          {\n            id: this.detailProduct[this.productIndex].id,\n            image_url: this.detailProduct[this.productIndex].image_url,\n            note: this.detailProduct[this.productIndex].note,\n            order_id: this.detailProduct[this.productIndex].order_id,\n            price: this.detailProduct[this.productIndex].price,\n            product_title: this.detailProduct[this.productIndex].product_title,\n            quantity: this.detailProduct[this.productIndex].quantity,\n            supplier_name: this.detailProduct[this.productIndex].supplier_name,\n            total_price: this.detailProduct[this.productIndex].total_price,\n            remove: this.detailProduct[this.productIndex].remove,\n          },\n        ],\n      };\n      Proxy.put(\n        uri + \"api/orders/:\" + this.$route.params.order_id,\n\n        body,\n        {\n          headers,\n        }\n      )\n        .then((res) => {\n          console.log(res);\n          this.snackbar.show = true;\n          //this.$router.push(\"/Menu/Order\");\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      console.log(body);\n    },\n  },\n  mounted() {\n    const uri = \"https://api-dev.phantasmode.com\";\n    // get token from localStorage\n    const token = localStorage.getItem(\"access_token\");\n    // set the headers\n    const headers = {\n      Authorization: \"Bearer \" + token,\n      Accept: \"application/json\",\n    };\n    Proxy.get(uri + \"/api/contacts\" + \"?type=1\", { headers })\n      .then((res) => {\n        this.dataCustomer = res.data.data;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n\n    Proxy.get(uri + \"/api/orders/\" + this.$route.params.order_id, { headers })\n      .then((res) => {\n        this.inputData.name_client = res.data.client_name;\n        this.inputData.type = res.data.type;\n        this.inputData.currency = res.data.currency;\n        this.inputData.date = res.data.order_date;\n        this.inputData.status = res.data.status;\n        this.valueProduct = res.data.items;\n        this.detailProduct = res.data.items;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  },\n};\n</script>\n\n<style scoped>\n.editWidth {\n  width: 50px;\n}\n</style>\n"],"sourceRoot":"src/views/dataOrder"}]}