                            </v-col>\n                            <v-col cols=\"12\">\n                              <v-list>\n                                <v-list-title\n                                  ><h3>Selected Product</h3></v-list-title\n                                >\n                                <v-list-item\n                                  v-for=\"(value, index) in valueProduct\"\n                                  :key=\"index\"\n                                >\n                                  <v-list-item-image>\n                                    <v-img\n                                      class=\"ma-2 pa-2\"\n                                      :src=\"value.image_url\"\n                                      max-width=\"40\"\n                                      max-height=\"40\"\n                                    >\n                                    </v-img>\n                                  </v-list-item-image>\n                                  <v-list-item-title\n                                    >Product:\n                                    {{ value.product_name }}\n                                    {{ value.product_title }}\n                                  </v-list-item-title>\n                                  <v-list-item-subtitle>\n                                    Price : {{ value.price }}\n                                    {{ value.currency }}\n                                  </v-list-item-subtitle>\n                                  <v-list-item-action>\n                                    <v-icon\n                                      slot=\"append\"\n                                      color=\"red\"\n                                      @click=\"removeProduct\"\n                                    >\n                                      mdi-minus-circle-outline\n                                    </v-icon>\n                                  </v-list-item-action>\n                                