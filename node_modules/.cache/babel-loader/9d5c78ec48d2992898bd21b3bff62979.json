\">\r\n                <v-text-field\r\n                  v-model=\"defaultItem.addresses.address_line_2\"\r\n                  label=\"Address Line 2\"\r\n                >\r\n                </v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"4\">\r\n                <v-text-field\r\n                  v-model=\"defaultItem.addresses.zipcode\"\r\n                  label=\"Postal Code\"\r\n                >\r\n                </v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"4\">\r\n                <v-select\r\n                  v-model=\"defaultItem.addresses.type\"\r\n                  label=\"Type\"\r\n                  :items=\"listData.typeclass\"\r\n                  item-text=\"name\"\r\n                  item-value=\"id\"\r\n                >\r\n                </v-select>\r\n              </v-col>\r\n              <v-col cols=\"4\"></v-col>\r\n              <v-col>\r\n                <v-btn small color=\"error\" @click=\"addAddress\"\r\n                  >Add Address</v-btn\r\n                >\r\n              </v-col>\r\n              <v-col cols=\"12\">\r\n                <span class=\"headline\">Social Accounts</span>\r\n              </v-col>\r\n              <v-col cols=\"12\">\r\n                <v-list>\r\n                  <div class=\"text-right\">Is Primary</div>\r\n                  <v-list-item\r\n                    v-for=\"(value, index) in valuesocialnetwork\"\r\n                    :key=\"index\"\r\n                  >\r\n                    <v-list-item-remove>\r\n                      <v-icon slot=\"prepend\" color=\"red\">\r\n                        mdi-minus-circle-outline\r\n                      </v-icon>\r\n                    </v-list-item-remove>\r\n                    <v-list-item-content>\r\n                      <h5>\r\n                        {{ value.url }}(<span\r\n                          class=\"font-weight-bold\"\r\n                          v-if=\"value.type == 1\"\r\n          