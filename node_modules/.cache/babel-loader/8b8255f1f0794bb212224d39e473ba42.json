A,IAAA,MAJA,oBAIA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,yBAAA;AACA,KANA;AAOA,IAAA,MAPA,kBAOA,EAPA,EAOA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,6BAAA,EAAA;AACA,KAVA;AAWA,IAAA,IAXA,gBAWA,EAXA,EAWA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,2BAAA,EAAA;AACA,KAdA;AAeA,IAAA,eAfA,6BAeA;AAAA;;AACA,WAAA,YAAA,GAAA,IAAA;AACA,UAAA,GAAA,GAAA,iCAAA,CAFA,CAGA;;AACA,UAAA,KAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA,CAJA,CAKA;;AALA,+BAMA,KAAA,YANA;AAAA,UAMA,IANA,sBAMA,IANA;AAAA,UAMA,YANA,sBAMA,YANA;AAOA,UAAA,OAAA,GAAA;AACA,QAAA,aAAA,EAAA,YAAA,KADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA;AAIA,MAAA,KAAA,CAAA,GAAA,CACA,GAAA,GAAA,aAAA,GAAA,QAAA,GAAA,IAAA,GAAA,SAAA,GAAA,YADA,EAEA;AAAA,QAAA,OAAA,EAAA;AAAA,OAFA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,OARA,EASA,KATA,CASA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,OAZA;AAaA;AAvCA,GAhCA;AA0EA,EAAA,OA1EA,qBA0EA;AACA,SAAA,eAAA;AACA;AA5EA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n    <v-data-table\r\n      :loading=\"loadingOrder\"\r\n      :headers=\"headers\"\r\n      :items=\"listOrder\"\r\n      item-key=\"id\"\r\n      :page=\"pageOrder\"\r\n      :options.sync=\"optionsOrder\"\r\n      :server-items-length=\"totalOrder\"\r\n      @update:options=\"paginate\"\r\n      show-select\r\n      class=\"elevation-1\"\r\n      loading-text=\"Loading . . . Please Wait\"\r\n    >\r\n      <template v-slot:top flat>\r\n        <v-toolbar class=\"elevation-1\">\r\n          <v-btn class=\"ma-2\" color=\"primary\" @click=\"create\"\r\n            ><v-icon>mdi-cart-plus</v-icon>Create Order</v-btn\r\n          >\r\n        </v-toolbar>\r\n      </template>\r\n\r\n      <template v-slot:[`item.type`]=\"items\">\r\n        <div class=\"justify-center\" v-for=\"(type, index) in items\" :key=\"index\">\r\n          <v-chip small color=\"primary\" v-if=\"type == 1\">income</v-chip>\r\n          <v-chip small v-if=\"type == 2\">outcome</v-chip>\r\n        </div>\r\n      </template>\r\n\r\n      <template v-slot:[`item.total_currency`]=\"{ item }\"\r\n        >{{ item.grand_total }} ({{ item.currency }})\r\n      </template>\r\n\r\n      <template v-slot:[`item.actions`]=\"{ item }\">\r\n        <v-menu>\r\n          <template v-slot:activator=\"{ on: menu }\">\r\n            <v-tooltip bottom>\r\n              <template v-slot:activator=\"{ on: tooltip }\">\r\n                <v-btn icon v-on=\"{ ...menu, ...tooltip }\">\r\n                  <v-icon>mdi-dots-vertical</v-icon></v-btn\r\n                >\r\n              </template>\r\n              <span>Action</span>\r\n            </v-tooltip>\r\n          </template>\r\n          <v-list class=\"pa-0\" dense>\r\n            <v-list-item @click=\"Edit(item.id)\">\r\n              <v-list-item-icon class=\"mr-2\">\r\n                <v-icon small>mdi-cart</v-icon>\r\n              </v-list-item-icon>\r\n              <v-list-item-title>Edit Order</v-list-item-title>\r\n            </v-list-item>\r\n            <v-list-item @click=\"Delete(item.id)\">\r\n              <v-list-item-icon class=\"mr-2\">\r\n                <v-icon small>mdi-cart-off</v-icon>\r\n              </v-list-item-icon>\r\n              <v-list-item-title>Delete Order</v-list-item-title>\r\n            </v-list-item>\r\n            <v-list-item @click=\"Detail(item.id)\">\r\n              <v-list-item-icon class=\"mr-2\">\r\n                <v-icon small>info</v-icon>\r\n              </v-list-item-icon>\r\n              <v-list-item-title>Detail Order</v-list-item-title>\r\n            </v-list-item>\r\n          </v-list>\r\n        </v-menu>\r\n      </template>\r\n    </v-data-table>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Proxy from \"@/services/proxy\";\r\nexport default {\r\n  name: \"listOrder\",\r\n  data() {\r\n    return {\r\n      loadingOrder: false,\r\n      headers: [\r\n        {\r\n          text: \"Order Code\",\r\n          align: \"start\",\r\n          sortable: false,\r\n          value: \"order_code\",\r\n        },\r\n        { text: \"Type\", value: \"type\" },\r\n        { text: \"Total(Currency)\", value: \"total_currency\" },\r\n        { text: \"Client Name\", value: \"client_name\" },\r\n        { text: \"Order Date\", value: \"order_date\" },\r\n        { text: \"Actions\", value: \"actions\", sortable: false },\r\n      ],\r\n      listOrder: [],\r\n      pageOrder: 1,\r\n      totalOrder: 0,\r\n      optionsOrder: {},\r\n    };\r\n  },\r\n  watch: {\r\n    options: {\r\n      handler() {\r\n        this.readDataFromAPI();\r\n      },\r\n    },\r\n    deep: true,\r\n  },\r\n  methods: {\r\n    paginate() {\r\n      this.readDataFromAPI();\r\n    },\r\n    create() {\r\n      this.$router.push(\"/Menu/Order/createOrder\");\r\n    },\r\n    Detail(id) {\r\n      console.log(id);\r\n      this.$router.push(\"/Menu/Order/detailOrder/\" + id);\r\n    },\r\n    Edit(id) {\r\n      console.log(id);\r\n      this.$router.push(\"/Menu/Order/editOrder/\" + id);\r\n    },\r\n    readDataFromAPI() {\r\n      this.loadingOrder = true;\r\n      const uri = \"https://api-dev.phantasmode.com\";\r\n      // get token from localStorage\r\n      const token = localStorage.getItem(\"access_token\");\r\n      // set the headers\r\n      const { page, itemsPerPage } = this.optionsOrder;\r\n      const headers = {\r\n        Authorization: \"Bearer \" + token,\r\n        Accept: \"application/json\",\r\n      };\r\n      Proxy.get(\r\n        uri + \"/api/orders\" + \"?page=\" + page + \"&limit=\" + itemsPerPage,\r\n        { headers }\r\n      )\r\n        .then((res) => {\r\n          this.listOrder = res.data.data;\r\n          this.totalOrder = res.data.total;\r\n          this.loadingOrder = false;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n          this.loadingOrder = false;\r\n        });\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.readDataFromAPI();\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/dataOrder"}]}