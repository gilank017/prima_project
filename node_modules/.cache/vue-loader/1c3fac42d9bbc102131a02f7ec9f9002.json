                       <th class=\"text-left\">image</th>\r\n                        <th class=\"text-left\">Product Name</th>\r\n                        <th class=\"text-left\">Price</th>\r\n                        <th class=\"text-left\">Quantity</th>\r\n                        <th class=\"text-left\">Total</th>\r\n                        <th class=\"text-left\">Actions</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"(value, index) in detailProduct\" :key=\"index\">\r\n                        <td>\r\n                          <v-img\r\n                            class=\"ma-2 pa-2\"\r\n                            :src=\"value.image_url\"\r\n                            max-width=\"70\"\r\n                            max-height=\"70\"\r\n                          ></v-img>\r\n                        </td>\r\n                        <td>\r\n                          <v-list>\r\n                            <v-list-item-title>\r\n                              <h3>{{ value.product_name }}</h3>\r\n                              <caption v-if=\"value.supplier_name != null\">\r\n                                By\r\n                                {{\r\n                                  value.supplier_name\r\n                                }}\r\n                              </caption>\r\n                            </v-list-item-title>\r\n                            <v-list-item-subtitle v-if=\"value.note != null\">\r\n                              Note : {{ value.note }}\r\n                            </v-list-item-subtitle>\r\n                            <v-list-item-subtitle>\r\n                              <v-btn\r\n                                text\r\n                                x-small\r\n                                @click=\"openNote(index)\"\r\n                                color=\"primary\"\r\n                                >Add Note</v-btn\r\n                              >\r\n                            </v-list-item-subtitle>\r\n                          </v-list>\r\n                        </td>\r\n                        <td>{{ value.price }}</td>\r\n                        <td>\r\n                          <v-text-field\r\n                            v-model=\"quantity\"\r\n                            class=\"editWidth\"\r\n                            type=\"number\"\r\n                            @change=\"calculateTotal(index)\"\r\n                          ></v-text-field>\r\n                        </td>\r\n\r\n                        <td>{{ totalProduct }}</td>\r\n                        <td>\r\n                          <v-icon @click=\"removeProduct\">clear</v-icon>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </template>\r\n                </v-simple-table>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-form>\r\n      </v-col>\r\n      <v-divider class=\"mt-5\"></v-divider>\r\n      <v-card-actions>\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"error\" @click=\"cancelData\"> Cancel </v-btn>\r\n        <v-btn color=\"primary\" @click=\"saveData(index)\"> Save </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Proxy from \"@/services/proxy\";\r\nexport default {\r\n  name: \"createOrder\",\r\n  data() {\r\n    return {\r\n      date: \"\",\r\n      menu: false,\r\n      dialog: false,\r\n      dialogNote: false,\r\n      dataCustomer: [],\r\n      typeOrder: [\r\n        { id: 1, name: \"Income\" },\r\n        { id: 2, name: \"Outcome\" },\r\n      ],\r\n      inputData: {\r\n        name_client: \"\",\r\n        type: \"\",\r\n        currency: \"\",\r\n        date: \"\",\r\n      },\r\n      snackbar: {\r\n        show: false,\r\n        text: \"Data Added Successfully\",\r\n        color: \"success\",\r\n      },\r\n      currencyList: [\"USD\", \"IDR\", \"CNY\"],\r\n      detailProduct: [],\r\n      dataProductList: [],\r\n      noteProduct: \"\",\r\n      selectProduct: \"\",\r\n      queryTerm: \"\",\r\n      valueProduct: [],\r\n      activeProduct: \"\",\r\n      quantity: \"\",\r\n      totalProduct: 0,\r\n    };\r\n  },\r\n  watch: {\r\n    search: {\r\n      get() {\r\n        this.queryTerm;\r\n      },\r\n      set(searchInput) {\r\n        if (this.queryTerm !== searchInput) {\r\n          this.queryTerm = searchInput;\r\n          this.loadProduct();\r\n        }\r\n      },\r\n    },\r\n  },\r\n  created() {\r\n    this.loadProduct();\r\n  },\r\n\r\n  methods: {\r\n    changeCurrency() {\r\n      this.loadProduct();\r\n    },\r\n\r\n    loadProduct() {\r\n      const uri = \"https://api-dev.phantasmode.com\";\r\n      // get token from localStorage\r\n      const token = localStorage.getItem(\"access_token\");\r\n      // set the headers\r\n      const headers = {\r\n        Authorization: \"Bearer \" + token,\r\n        Accept: \"application/json\",\r\n      };\r\n\r\n      Proxy.get(\r\n        uri +\r\n          \"/api/products/\" +\r\n          \"?product_name=\" +\r\n          (this.queryTerm || \"\") +\r\n          \"&convertTo=\" +\r\n          this.inputData.currency,\r\n        { headers }\r\n      )\r\n        .then((res) => {\r\n          this.dataProduct = res.data;\r\n          this.queryTerm = res.data.total;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    addProduct() {\r\n      const incomingDataProduct = this.selectProduct;\r\n      this.selectProduct = {};\r\n      this.valueProduct.push(incomingDataProduct);\r\n    },\r\n    close() {\r\n      this.dialog = false;\r\n      this.dialogNote = false;\r\n      this.selectProduct = null;\r\n    },\r\n    save() {\r\n      this.detailProduct = this.valueProduct;\r\n      console.log(this.detailProduct);\r\n      this.close();\r\n    },\r\n    removeProduct(index) {\r\n      this.valueProduct.splice(index, 1);\r\n    },\r\n    openNote(indexProduct) {\r\n      this.activeProduct = indexProduct;\r\n      this.dialogNote = true;\r\n    },\r\n    saveNote() {\r\n      this.detailProduct[this.activeProduct].note = this.noteProduct;\r\n      this.noteProduct = \"\";\r\n      this.dialogNote = false;\r\n    },\r\n    calculateTotal(index) {\r\n      var total = this.detailProduct[index].price * this.quantity;\r\n      this.totalProduct = total;\r\n      console.log(total);\r\n    },\r\n    cancelData() {\r\n      this.$router.push(\"/Menu/Order/\");\r\n    },\r\n    saveData() {\r\n      const uri = \"https://api-dev.phantasmode.com\";\r\n      // get token from localStorage\r\n      const token = localStorage.getItem(\"access_token\");\r\n      // set the headers\r\n      const headers = {\r\n        Authorization: \"Bearer \" + token,\r\n        Accept: \"application/json\",\r\n      };\r\n      const body = {\r\n        type: this.inputData.type,\r\n        client_name: this.inputData.name_client,\r\n        currency: this.inputData.currency,\r\n        order_date: this.inputData.date,\r\n        items: [\r\n          {\r\n            product_title: this.detailProduct[0].product_name,\r\n            quantity: this.quantity,\r\n            price: this.detailProduct[0].price,\r\n            image_url: this.detailProduct[0].image_url,\r\n            note: this.detailProduct[0].note,\r\n          },\r\n        ],\r\n      };\r\n      Proxy.post(uri + \"/api/orders\", body, { headers })\r\n        .then((res) => {\r\n          console.log(res);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      console.log(body);\r\n      this.snackbar.show = true;\r\n      this.$router.push(\"/Menu/Order\");\r\n    },\r\n  },\r\n  mounted() {\r\n    const uri = \"https://api-dev.phantasmode.com\";\r\n    // get token from localStorage\r\n    const token = localStorage.getItem(\"access_token\");\r\n    // set the headers\r\n    const headers = {\r\n      Authorization: \"Bearer \" + token,\r\n      Accept: \"application/json\",\r\n    };\r\n    Proxy.get(uri + \"/api/contacts\" + \"?type=1\", { headers })\r\n      .then((res) => {\r\n        this.dataCustomer = res.data.data;\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.editWidth {\r\n  width: 50px;\r\n}\r\n</style>\r\n"]}]}