ield\r\n                          v-model=\"defaultItem.addresses.zipcode\"\r\n                          label=\"Postal Code\"\r\n                        >\r\n                        </v-text-field>\r\n                      </v-col>\r\n                      <v-col cols=\"4\">\r\n                        <v-select\r\n                          v-model=\"defaultItem.addresses.type\"\r\n                          label=\"Type\"\r\n                          :items=\"listData.typeclass\"\r\n                          item-text=\"name\"\r\n                          :value=\"val\"\r\n                        >\r\n                        </v-select>\r\n                      </v-col>\r\n                      <v-col cols=\"4\"></v-col>\r\n                      <v-col>\r\n                        <v-btn small color=\"error\" @click=\"addAddress\"\r\n                          >Add Address</v-btn\r\n                        >\r\n                      </v-col>\r\n                      <v-col cols=\"12\">\r\n                        <span class=\"headline\">Social Accounts</span>\r\n                      </v-col>\r\n                      <v-col cols=\"12\">\r\n                        <v-list>\r\n                          <div class=\"text-right\">Is Primary</div>\r\n                          <v-list-item\r\n                            v-for=\"(value, index) in valuesocialnetwork\"\r\n                            :key=\"index\"\r\n                          >\r\n                            <v-list-item-remove>\r\n                              <v-icon slot=\"prepend\" color=\"red\">\r\n                                mdi-minus-circle-outline\r\n                              </v-icon>\r\n                            </v-list-item-remove>\r\n                            <v-list-item-content>\r\n                              <h5>{{ value.url }}({{ value.type }})</h5>\r\n                            </v-list-item-content>\r\n                            <v-list-item-action>\r\n                              <v-radio-group v-model=\"value.is_primary\">\r\n                                <v-radio :key=\"index\" :value=\"1\"> </v-radio>\r\n                              </v-radio-group>\r\n                            </v-list-item-action>\r\n                          </v-list-item>\r\n                        </v-list>\r\n                      </v-col>\r\n                      <v-col cols=\"4\">\r\n                        <v-select\r\n                          v-model=\"defaultItem.socialnetwork.type\"\r\n                          label=\"Type\"\r\n                          :items=\"listData.typeclass\"\r\n                          item-text=\"name\"\r\n                        >\r\n                        </v-select>\r\n                      </v-col>\r\n                      <v-col cols=\"8\">\r\n                        <v-text-field\r\n                          v-model=\"defaultItem.socialnetwork.url\"\r\n                          label=\"url\"\r\n                          :append-icon=\"'mdi-email-plus-outline'\"\r\n                          @click:append=\"addsocialaccount\"\r\n                        ></v-text-field>\r\n                      </v-col>\r\n                    </v-row>\r\n                  </v-form>\r\n                </v-container>\r\n              </v-card-text>\r\n              <v-divider class=\"mt-5\"></v-divider>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"error\" @click=\"close\"> Cancel </v-btn>\r\n                <v-btn color=\"primary\" @click=\"save\"> Save </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n            <v-card>\r\n              <v-card-title class=\"headline\"\r\n                >Are you sure you want to delete this item?</v-card-title\r\n              >\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"closeDelete\"\r\n                  >Cancel</v-btn\r\n                >\r\n                <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\"\r\n                  >OK</v-btn\r\n                >\r\n                <v-spacer></v-spacer>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-dialog v-model=\"dialogDetail\" max-width=\"600px\">\r\n            <v-card>\r\n              <v-card-title class=\"headline\">Detail Contact</v-card-title>\r\n              <v-card-text>\r\n                <v-text></v-text>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"close\">close</v-btn>\r\n                >\r\n                <v-spacer></v-spacer>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-toolbar>\r\n      </template>\r\n      <template v-slot:[`item.actions`]=\"{ item }\">\r\n        <v-menu>\r\n          <template v-slot:activator=\"{ on: menu }\">\r\n            <v-tooltip bottom>\r\n              <template v-slot:activator=\"{ on: tooltip }\">\r\n                <v-btn icon v-on=\"{ ...menu, ...tooltip }\">\r\n                  <v-icon>mdi-dots-vertical</v-icon></v-btn\r\n                >\r\n              </template>\r\n              <span>Action</span>\r\n            </v-tooltip>\r\n          </template>\r\n          <v-list class=\"pa-0\" dense>\r\n            <v-list-item\r\n              v-for=\"(actions, index) in actions\"\r\n              :key=\"index\"\r\n              @click=\"actions.click(item)\"\r\n            >\r\n              <v-list-item-icon class=\"mr-2\">\r\n                <v-icon small>{{ actions.icon }}</v-icon>\r\n              </v-list-item-icon>\r\n              <v-list-item-title>{{ actions.text }}</v-list-item-title>\r\n            </v-list-item>\r\n          </v-list>\r\n        </v-menu>\r\n      </template>\r\n      <template v-slot:[`item.type`]=\"items\">\r\n        <v-text v-for=\"(type, index) in items\" :key=\"index\">\r\n          <td v-if=\"type == 1\">customer</td>\r\n          <td v-if=\"type == 2\">supplier</td>\r\n        </v-text>\r\n      </template>\r\n    </v-data-table>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Proxy from \".../services/proxy\";\r\nimport validations from \"@/utils/validations\";\r\nexport default {\r\n  name: \"Contact\",\r\n  data: () => ({\r\n    dialog: false,\r\n    dialogDetail: false,\r\n    headers: [\r\n      {\r\n        text: \"Name\",\r\n        align: \"start\",\r\n        sortable: false,\r\n        value: \"name\",\r\n      },\r\n      { text: \"Type\", value: \"type\" },\r\n      { text: \"Email\", value: \"primary_email\" },\r\n      { text: \"Phone\", value: \"primary_phone\" },\r\n      { text: \"Data Added\", value: \"created_at\" },\r\n      { text: \"Actions\", value: \"actions\", sortable: false },\r\n    ],\r\n    actions: [\r\n      {\r\n        text: \"Edit Item\",\r\n        icon: \"mdi-account-edit\",\r\n        click: (data) => {\r\n          console.log(\"edit\", data);\r\n        },\r\n      },\r\n      {\r\n        text: \"Delete Item\",\r\n        icon: \"mdi-account-remove\",\r\n      },\r\n      {\r\n        text: \"Detail Item\",\r\n        icon: \"mdi-account-settings\",\r\n        click: (data) => {\r\n          console.log(\"show\", data);\r\n          this.valueData = \"show\".data;\r\n          this.dialogDetail = true;\r\n        },\r\n      },\r\n    ],\r\n    list: [],\r\n    editedIndex: -1,\r\n    defaultItem: {\r\n      name: \"\",\r\n      type: \"\",\r\n      emails: [{ type: \"\" }, { is_primary: 0 }, { email: \"\" }],\r\n      phones: [\r\n        { type: \"\" },\r\n        { is_primary: 0 },\r\n        { country_code: \"\" },\r\n        { number: \"\" },\r\n      ],\r\n      addresses: [\r\n        { type: \"\" },\r\n        { is_primary: 0 },\r\n        { address_line_1: \"\" },\r\n        { address_line_2: \"\" },\r\n        { city: \"\" },\r\n        { country: \"\" },\r\n        { state: \"\" },\r\n        { zipcode: \"\" },\r\n      ],\r\n      socialnetwork: [{ type: \"\" }, { is_primary: 0 }, { url: \"\" }],\r\n    },\r\n    ...validations,\r\n    listData: {\r\n      typeData: [\"1\", \"2\"],\r\n      typeclass: [\r\n        { name: \"Personal\", val: \"1\" },\r\n        { name: \"Work\", val: \"2\" },\r\n        { name: \"Other\", val: \"3\" },\r\n      ],\r\n      typeCountry: [\"Indonesia\", \"English\", \"Arabic\"],\r\n      typecountry_code: [\"ID\", \"US\", \"AS\"],\r\n    },\r\n    valueEmail: [],\r\n    valuePhone: [],\r\n    valueAddress: [],\r\n    valuesocialnetwork: [],\r\n    valueData: null,\r\n  }),\r\n\r\n  computed: {\r\n    formTitle() {\r\n      return this.editedIndex === -1 ? \"New Item\" : \"Edit Item\";\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    dialog(val) {\r\n      val || this.close();\r\n    },\r\n    dialogDelete(val) {\r\n      val || this.closeDelete();\r\n    },\r\n    dialogDetail(val) {\r\n      val || this.dialogDetail();\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    initialize() {\r\n      this.list = [];\r\n    },\r\n\r\n    close() {\r\n      this.dialog = false;\r\n    },\r\n\r\n    addemail() {\r\n      const incomingDataEmail = {\r\n        type: this.defaultItem.emails.type,\r\n        is_primary: this.defaultItem.emails.is_primary,\r\n        email: this.defaultItem.emails.email,\r\n      };\r\n      this.defaultItem.emails = {\r\n        type: \"\",\r\n        is_primary: \"0\",\r\n        email: \"\",\r\n      };\r\n      this.valueEmail.push(incomingDataEmail);\r\n    },\r\n\r\n    addphone() {\r\n      const incomingDataPhone = {\r\n        type: this.defaultItem.phones.type,\r\n        is_primary: this.defaultItem.phones.is_primary,\r\n        country_code: this.defaultItem.phones.country_code,\r\n        number: this.defaultItem.phones.number,\r\n      };\r\n      this.defaultItem.phones = {\r\n        type: \"\",\r\n        is_primary: \"0\",\r\n        country_code: \"\",\r\n        number: \"\",\r\n      };\r\n      this.valuePhone.push(incomingDataPhone);\r\n    },\r\n    addAddress() {\r\n      const incomingDataAddress = {\r\n        type: this.defaultItem.addresses.type,\r\n        is_primary: this.defaultItem.addresses.is_primary,\r\n        address_line_1: this.defaultItem.addresses.address_line_1,\r\n        address_line_2: this.defaultItem.addresses.address_line_2,\r\n        city: this.defaultItem.addresses.city,\r\n        country: this.defaultItem.addresses.country,\r\n        state: this.defaultItem.addresses.state,\r\n        zipcode: this.defaultItem.addresses.zipcode,\r\n      };\r\n      this.defaultItem.addresses = {\r\n        type: \"\",\r\n        is_primary: \"0\",\r\n        address_line_1: \"\",\r\n        address_line_2: \"\",\r\n        city: \"\",\r\n        country: \"\",\r\n        state: \"\",\r\n        zipcode: \"\",\r\n      };\r\n      this.valueAddress.push(incomingDataAddress);\r\n    },\r\n    addsocialaccount() {\r\n      const incomingDataEmail = {\r\n        type: this.defaultItem.socialnetwork.type,\r\n        is_primary: this.defaultItem.socialnetwork.is_primary,\r\n        url: this.defaultItem.socialnetwork.url,\r\n      };\r\n      this.defaultItem.socialnetwork = {\r\n        type: \"\",\r\n        is_primary: \"0\",\r\n        url: \"\",\r\n      };\r\n      this.valuesocialnetwork.push(incomingDataEmail);\r\n    },\r\n    save() {\r\n      const uri = \"https://api-dev.phantasmode.com\";\r\n      const token = localStorage.getItem(\"access_token\");\r\n      const headers = {\r\n        Authorization: \"Bearer \" + token,\r\n        Accept: \"application/json\",\r\n      };\r\n      const body = {\r\n        name: this.defaultItem.name,\r\n        type: this.defaultItem.type,\r\n        emails: this.valueEmail,\r\n        phones: this.valuePhone,\r\n        addresses: this.valueAddress,\r\n        socialnetwork: this.valuesocialnetwork,\r\n      };\r\n      Proxy.post(uri + \"/api/contacts\", body, { headers })\r\n        .then((response) => {\r\n          console.log(response.data);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    detailItem() {\r\n      const uri = \"https://api-dev.phantasmode.com\";\r\n      const token = localStorage.getItem(\"access_token\");\r\n      const headers = {\r\n        Authorization: \"Bearer \" + token,\r\n        Accept: \"application/json\",\r\n      };\r\n      Proxy.post(uri + \"/api/contacts/:Id\", { headers })\r\n        .then((show) => {\r\n          console.log(show.data.valueData);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n      this.dialogDetail = true;\r\n    },\r\n  },\r\n  async mounted() {\r\n    // set url\r\n    const uri = \"https://api-dev.phantasmode.com\";\r\n    // get token from localStorage\r\n    const token = localStorage.getItem(\"access_token\");\r\n    // set the headers\r\n    const headers = {\r\n      Authorization: \"Bearer \" + token,\r\n    };\r\n    const response = await Proxy.get(uri + \"/api/contacts\", { headers });\r\n    console.log(response.data.data);\r\n    this.list = response.data.data;\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views/Contact"}]}