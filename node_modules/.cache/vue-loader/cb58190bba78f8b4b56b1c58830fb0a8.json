     <th class=\"text-left\">image</th>\n                        <th class=\"text-left\">Product Name</th>\n                        <th class=\"text-left\">Price</th>\n                        <th class=\"text-left\">Quantity</th>\n                        <th class=\"text-left\">Total</th>\n                        <th class=\"text-left\">Actions</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      <tr v-for=\"(value, index) in detailProduct\" :key=\"index\">\n                        <td>\n                          <v-img\n                            class=\"ma-2 pa-2\"\n                            :src=\"value.image_url\"\n                            max-width=\"70\"\n                            max-height=\"70\"\n                          ></v-img>\n                        </td>\n                        <td>\n                          <v-list>\n                            <v-list-item-title>\n                              <h3>\n                                {{ value.product_title }}\n                                {{ value.product_name }}\n                              </h3>\n                              <caption v-if=\"value.supplier_name != null\">\n                                By\n                                {{\n                                  value.supplier_name\n                                }}\n                              </caption>\n                            </v-list-item-title>\n                            <v-list-item-subtitle v-if=\"value.note != null\">\n                              Note : {{ value.note }}\n                            </v-list-item-subtitle>\n                            <v-list-item-subtitle>\n                              <v-btn\n                                text\n                                x-small\n                                @click=\"openNote(index)\"\n                                color=\"primary\"\n                                >Add Note</v-btn\n                              >\n                            </v-list-item-subtitle>\n                          </v-list>\n                        </td>\n                        <td>{{ value.price }}</td>\n                        <td>\n                          <v-text-field\n                            v-model=\"value.quantity\"\n                            class=\"editWidth\"\n                            type=\"number\"\n                            @change=\"calculateTotal(index)\"\n                          ></v-text-field>\n                        </td>\n\n                        <td>{{ value.total_price }}</td>\n                        <td>\n                          <v-icon\n                            v-model=\"value.remove\"\n                            @click=\"removeDetailProduct\"\n                            >clear</v-icon\n                          >\n                        </td>\n                      </tr>\n                    </tbody>\n                  </template>\n                </v-simple-table>\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-form>\n      </v-col>\n      <v-divider class=\"mt-5\"></v-divider>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"error\" @click=\"cancelData\"> Cancel </v-btn>\n        <v-btn color=\"primary\" @click=\"saveData\"> Save </v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-app>\n</template>\n\n<script>\nimport Proxy from \"@/services/proxy\";\nexport default {\n  name: \"editOrder\",\n  data() {\n    return {\n      date: \"\",\n      menu: false,\n      dialog: false,\n      dialogNote: false,\n      dataCustomer: [],\n      typeOrder: [\n        { id: 1, name: \"Income\" },\n        { id: 2, name: \"Outcome\" },\n      ],\n      inputData: {\n        name_client: \"\",\n        type: \"\",\n        currency: \"\",\n        date: \"\",\n        status: \"\",\n      },\n      snackbar: {\n        show: false,\n        text: \"Data Added Successfully\",\n        color: \"success\",\n      },\n      currencyList: [\"USD\", \"IDR\", \"CNY\"],\n      statusList: [\n        { id: 1, name: \"Pending\" },\n        { id: 2, name: \"Delivered\" },\n      ],\n      detailProduct: [],\n      detailProductList: {\n        id: \"\",\n        image_url: \"\",\n        note: \"\",\n        order_id: \"\",\n        price: \"\",\n        product_title: \"\",\n        quantity: \"\",\n        supplier_name: \"\",\n        total_price: \"\",\n        updated_at: \"\",\n      },\n      noteProduct: \"\",\n      selectProduct: {},\n      queryTerm: \"\",\n      valueProduct: {},\n      activeProduct: \"\",\n      productIndex: 0,\n      quantity: \"\",\n      totalProduct: 0,\n    };\n  },\n  watch: {\n    search: {\n      get() {\n        this.queryTerm;\n      },\n      set(searchInput) {\n        if (this.queryTerm !== searchInput) {\n          this.queryTerm = searchInput;\n          this.loadProduct();\n        }\n      },\n    },\n  },\n  created() {\n    this.loadProduct();\n  },\n\n  methods: {\n    changeCurrency() {\n      this.loadProduct();\n    },\n\n    loadProduct() {\n      const uri = \"https://api-dev.phantasmode.com\";\n      // get token from localStorage\n      const token = localStorage.getItem(\"access_token\");\n      // set the headers\n      const headers = {\n        Authorization: \"Bearer \" + token,\n        Accept: \"application/json\",\n      };\n\n      Proxy.get(\n        uri +\n          \"/api/products/\" +\n          \"?product_name=\" +\n          (this.queryTerm || \"\") +\n          \"&convertTo=\" +\n          this.inputData.currency,\n        { headers }\n      )\n        .then((res) => {\n          this.dataProduct = res.data.data;\n          this.queryTerm = res.data.total;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n\n    addProduct() {\n      const incomingDataProduct = this.selectProduct;\n      this.selectProduct = {};\n      this.valueProduct.push(incomingDataProduct);\n    },\n    close() {\n      this.dialog = false;\n      this.dialogNote = false;\n      this.selectProduct = null;\n    },\n    save() {\n      this.detailProduct = {\n        id: this.valueProduct.id,\n        image_url: this.valueProduct.image_url,\n        order_id: this.valueProduct.order_id,\n        price: this.valueProduct.price,\n        product_title: this.valueProduct.product_name,\n        quantity: this.valueProduct.quantity,\n        supplier_name: this.valueProduct.supplier_name,\n      };\n      this.detailProduct = {\n        id: \"\",\n        image_url: \"\",\n        order_id: \"\",\n        price: \"\",\n        product_title: \"\",\n        quantity: \"\",\n        supplier_name: \"\",\n      };\n      this.valueProduct.push(this.detailProduct);\n\n      console.log(this.detailProduct);\n      this.close();\n    },\n    removeProduct(index) {\n      this.valueProduct.splice(index, 1);\n    },\n    removeDetailProduct(index) {\n      this.detailProduct.splice(index, 1);\n      this.detailProduct.remove = true;\n    },\n    openNote(indexProduct) {\n      this.activeProduct = indexProduct;\n      this.dialogNote = true;\n    },\n    saveNote() {\n      this.detailProduct[this.activeProduct].note = this.noteProduct;\n      this.noteProduct = \"\";\n      this.dialogNote = false;\n    },\n    calculateTotal() {\n      var total = this.detailProduct.price * this.detailProduct.quantity;\n      this.totalProduct = total;\n      console.log(total);\n    },\n    cancelData() {\n      this.$router.push(\"/Menu/Order/\");\n    },\n    saveData() {\n      const uri = \"https://api-dev.phantasmode.com\";\n      // get token from localStorage\n      const token = localStorage.getItem(\"access_token\");\n      // set the headers\n      const headers = {\n        Authorization: \"Bearer \" + token,\n        Accept: \"application/json\",\n      };\n      const body = {\n        type: this.inputData.type,\n        client_name: this.inputData.name_client,\n        currency: this.inputData.currency,\n        order_date: this.inputData.date,\n        status: this.inputData.status,\n        items: this.detailProduct,\n      };\n\n      Proxy.put(uri + \"/api/orders/\" + this.$route.params.order_id, body, {\n        headers,\n      })\n        .then((res) => {\n          console.log(res);\n          this.snackbar.show = true;\n          //this.$router.push(\"/Menu/Order\");\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      console.log(body);\n    },\n  },\n  mounted() {\n    const uri = \"https://api-dev.phantasmode.com\";\n    // get token from localStorage\n    const token = localStorage.getItem(\"access_token\");\n    // set the headers\n    const headers = {\n      Authorization: \"Bearer \" + token,\n      Accept: \"application/json\",\n    };\n    Proxy.get(uri + \"/api/contacts\" + \"?type=1\", { headers })\n      .then((res) => {\n        this.dataCustomer = res.data.data;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n\n    Proxy.get(uri + \"/api/orders/\" + this.$route.params.order_id, { headers })\n      .then((res) => {\n        this.inputData.name_client = res.data.client_name;\n        this.inputData.type = res.data.type;\n        this.inputData.currency = res.data.currency;\n        this.inputData.date = res.data.order_date;\n        this.inputData.status = res.data.status;\n        this.valueProduct = res.data.items;\n        this.detailProduct = res.data.items;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  },\n};\n</script>\n\n<style scoped>\n.editWidth {\n  width: 50px;\n}\n</style>\n\n[ { id: this.detailProduct[this.productIndex].id, image_url:\nthis.detailProduct[this.productIndex].image_url, note:\nthis.detailProduct[this.productIndex].note, order_id:\nthis.detailProduct[this.productIndex].order_id, price:\nthis.detailProduct[this.productIndex].price, product_title:\nthis.detailProduct[this.productIndex].product_title, quantity:\nthis.detailProduct[this.productIndex].quantity, supplier_name:\nthis.detailProduct[this.productIndex].supplier_name, total_price:\nthis.detailProduct[this.productIndex].total_price, remove:\nthis.detailProduct[this.productIndex].remove, }, ],\n"]}]}