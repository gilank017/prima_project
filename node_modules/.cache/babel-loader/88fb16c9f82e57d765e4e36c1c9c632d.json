,GAAA,CAAA,KAAA;AACA,KAhEA;AAiEA,IAAA,UAjEA,wBAiEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,cAAA;AACA,KAnEA;AAoEA,IAAA,QApEA,sBAoEA;AACA,UAAA,GAAA,GAAA,iCAAA,CADA,CAEA;;AACA,UAAA,KAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA,CAHA,CAIA;;AACA,UAAA,OAAA,GAAA;AACA,QAAA,aAAA,EAAA,YAAA,KADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA;AAIA,UAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,SAAA,CAAA,IADA;AAEA,QAAA,WAAA,EAAA,KAAA,SAAA,CAAA,WAFA;AAGA,QAAA,QAAA,EAAA,KAAA,SAAA,CAAA,QAHA;AAIA,QAAA,UAAA,EAAA,KAAA,SAAA,CAAA,IAJA;AAKA,QAAA,KAAA,EAAA,CACA;AACA,UAAA,aAAA,EAAA,KAAA,aAAA,CAAA,CAAA,EAAA,YADA;AAEA,UAAA,QAAA,EAAA,KAAA,QAFA;AAGA,UAAA,KAAA,EAAA,KAAA,aAAA,CAAA,CAAA,EAAA,KAHA;AAIA,UAAA,SAAA,EAAA,KAAA,aAAA,CAAA,CAAA,EAAA,SAJA;AAKA,UAAA,IAAA,EAAA,KAAA,aAAA,CAAA,CAAA,EAAA;AALA,SADA;AALA,OAAA;AAeA,MAAA,KAAA,CAAA,IAAA,CAAA,GAAA,GAAA,aAAA,EAAA,IAAA,EAAA;AAAA,QAAA,OAAA,EAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OANA;AAOA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,WAAA,QAAA,CAAA,IAAA,GAAA,IAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,aAAA;AACA;AAtGA,GArDA;AA6JA,EAAA,OA7JA,qBA6JA;AAAA;;AACA,QAAA,GAAA,GAAA,iCAAA,CADA,CAEA;;AACA,QAAA,KAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA,CAHA,CAIA;;AACA,QAAA,OAAA,GAAA;AACA,MAAA,aAAA,EAAA,YAAA,KADA;AAEA,MAAA,MAAA,EAAA;AAFA,KAAA;AAIA,IAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,eAAA,GAAA,SAAA,EAAA;AAAA,MAAA,OAAA,EAAA;AAAA,KAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAHA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KANA;AAOA;AA7KA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n    <v-snackbar\r\n      v-model=\"snackbar.show\"\r\n      :timeout=\"3000\"\r\n      top\r\n      centered\r\n      :color=\"snackbar.color\"\r\n    >\r\n      {{ snackbar.text }}\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn icon v-bind=\"attrs\" @click=\"snackbar.show = false\">\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n      </template>\r\n    </v-snackbar>\r\n    <v-card>\r\n      <v-col cols=\"12\">\r\n        <v-card-title>Customer</v-card-title>\r\n        <v-form>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"3\">\r\n                <v-select\r\n                  label=\"Type\"\r\n                  v-model=\"inputData.type\"\r\n                  :items=\"typeOrder\"\r\n                  item-text=\"name\"\r\n                  item-value=\"id\"\r\n                ></v-select>\r\n              </v-col>\r\n              <v-col cols=\"4\"></v-col>\r\n              <v-col cols=\"5\" md=\"3\">\r\n                <v-select\r\n                  label=\"Currency\"\r\n                  v-model=\"inputData.currency\"\r\n                  :items=\"currencyList\"\r\n                  @change=\"changeCurrency\"\r\n                ></v-select>\r\n              </v-col>\r\n              <v-col cols=\"5\">\r\n                <v-autocomplete\r\n                  label=\"Customer Name\"\r\n                  v-model=\"inputData.name_client\"\r\n                  :items=\"dataCustomer\"\r\n                  item-text=\"name\"\r\n                  item-value=\"name\"\r\n                >\r\n                </v-autocomplete>\r\n              </v-col>\r\n              <v-col cols=\"2\"></v-col>\r\n              <v-col cols=\"5\" md=\"3\">\r\n                <v-menu>\r\n                  <template v-slot:activator=\"{ on, attrs }\">\r\n                    <v-text-field\r\n                      label=\"Order Date\"\r\n                      append-icon=\"mdi-calendar\"\r\n                      readonly\r\n                      v-bind=\"attrs\"\r\n                      v-on=\"on\"\r\n                      :value=\"inputData.date\"\r\n                    ></v-text-field>\r\n                  </template>\r\n                  <v-date-picker v-model=\"inputData.date\" landscape>\r\n                  </v-date-picker>\r\n                </v-menu>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-form>\r\n      </v-col>\r\n      <v-col cols=\"12\">\r\n        <v-card-title>Create Orders</v-card-title>\r\n        <v-form>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"8\">\r\n                <v-list-title><h3>Detail Product</h3></v-list-title>\r\n              </v-col>\r\n              <v-col cols=\"4\">\r\n                <v-dialog v-model=\"dialog\" max-width=\"700px\">\r\n                  <template v-slot:activator=\"{ on, attrs }\">\r\n                    <v-btn class=\"ma-2\" color=\"primary\" v-on=\"on\" v-bind=\"attrs\"\r\n                      ><v-icon>mdi-plus</v-icon>Add Product</v-btn\r\n                    >\r\n                  </template>\r\n                  <v-card tile>\r\n                    <v-card-title>\r\n                      <span class=\"headline\">Add Product</span>\r\n                    </v-card-title>\r\n                    <v-card-text>\r\n                      <v-container>\r\n                        <v-form>\r\n                          <v-row>\r\n                            <v-col cols=\"12\" md=\"8\">\r\n                              <v-autocomplete\r\n                                v-model=\"selectProduct\"\r\n                                label=\"Search Product\"\r\n                                :items=\"dataProduct\"\r\n                                :search-input.sync=\"searchInput\"\r\n                                item-text=\"product_name\"\r\n                                item-value=\"product_name\"\r\n                                return-object\r\n                              >\r\n                                <v-btn\r\n                                  small\r\n                                  slot=\"append-outer\"\r\n                                  @click=\"addProduct\"\r\n                                  color=\"primary\"\r\n                                >\r\n                                  Add Product\r\n                                </v-btn>\r\n                                <template v-slot:item=\"data\">\r\n                                  <template\r\n                                    v-if=\"typeof data.item !== 'object'\"\r\n                                  >\r\n                                    <v-list-item-content\r\n                                      v-text=\"data.item\"\r\n                                    ></v-list-item-content>\r\n                                  </template>\r\n                                  <template v-else>\r\n                                    <v-list-item-image>\r\n                                      <v-img\r\n                                        class=\"ma-2 pa-2\"\r\n                                        :src=\"data.item.image_url\"\r\n                                        max-width=\"40\"\r\n                                        max-height=\"40\"\r\n                                      >\r\n                                      </v-img>\r\n                                    </v-list-item-image>\r\n                                    <v-list-item-content>\r\n                                      <v-list-item-title>{{\r\n                                        data.item.product_name\r\n                                      }}</v-list-item-title>\r\n                                      <v-list-item-sub-title\r\n                                        >{{ data.item.price }}\r\n                                        {{\r\n                                          data.item.currency\r\n                                        }}</v-list-item-sub-title\r\n                                      >\r\n                                    </v-list-item-content>\r\n                                  </template>\r\n                                </template>\r\n                              </v-autocomplete>\r\n                            </v-col>\r\n                            <v-col cols=\"12\">\r\n                              <v-list>\r\n                                <v-list-title\r\n                                  ><h3>Selected Product</h3></v-list-title\r\n                                >\r\n                                <v-list-item\r\n                                  v-for=\"(value, index) in valueProduct\"\r\n                                  :key=\"index\"\r\n                                >\r\n                                  <v-list-item-image>\r\n                                    <v-img\r\n                                      class=\"ma-2 pa-2\"\r\n                                      :src=\"value.image_url\"\r\n                                      max-width=\"40\"\r\n                                      max-height=\"40\"\r\n                                    >\r\n                                    </v-img>\r\n                                  </v-list-item-image>\r\n                                  <v-list-item-title\r\n                                    >Product:\r\n                                    {{ value.product_name }}</v-list-item-title\r\n                                  >\r\n                                  <v-list-item-subtitle>\r\n                                    Price : {{ value.price }}\r\n                                    {{ value.currency }}\r\n                                  </v-list-item-subtitle>\r\n                                  <v-list-item-action>\r\n                                    <v-icon\r\n                                      slot=\"append\"\r\n                                      color=\"red\"\r\n                                      @click=\"removeProduct\"\r\n                                    >\r\n                                      mdi-minus-circle-outline\r\n                                    </v-icon>\r\n                                  </v-list-item-action>\r\n                                </v-list-item>\r\n                              </v-list>\r\n                            </v-col>\r\n                          </v-row>\r\n                        </v-form>\r\n                      </v-container>\r\n                    </v-card-text>\r\n                    <v-divider class=\"mt-5\"></v-divider>\r\n                    <v-card-actions>\r\n                      <v-spacer></v-spacer>\r\n                      <v-btn color=\"error\" @click=\"close\"> Cancel </v-btn>\r\n                      <v-btn color=\"primary\" @click=\"save\"> Save </v-btn>\r\n                    </v-card-actions>\r\n                  </v-card>\r\n                </v-dialog>\r\n                <v-dialog v-model=\"dialogNote\" max-width=\"600px\" scrollable>\r\n                  <v-card tile>\r\n                    <v-card-title>\r\n                      <span class=\"headline\">Add Note</span>\r\n                    </v-card-title>\r\n                    <v-card-text>\r\n                      <v-container>\r\n                        <v-form>\r\n                          <v-row>\r\n                            <v-textarea\r\n                              v-model=\"noteProduct\"\r\n                              placeholder=\"Add Some Note. . .\"\r\n                            ></v-textarea>\r\n                          </v-row>\r\n                        </v-form>\r\n                      </v-container>\r\n                    </v-card-text>\r\n                    <v-divider class=\"mt-5\"></v-divider>\r\n                    <v-card-actions>\r\n                      <v-spacer></v-spacer>\r\n                      <v-btn color=\"error\" @click=\"close\"> Cancel </v-btn>\r\n                      <v-btn color=\"primary\" @click=\"saveNote\"> Save </v-btn>\r\n                    </v-card-actions>\r\n                  </v-card>\r\n                </v-dialog>\r\n              </v-col>\r\n              <v-col cols=\"12\">\r\n                <v-simple-table>\r\n                  <template v-slot:default>\r\n                    <thead>\r\n                      <tr>\r\n                        <th class=\"text-left\">image</th>\r\n                        <th class=\"text-left\">Product Name</th>\r\n                        <th class=\"text-left\">Price</th>\r\n                        <th class=\"text-left\">Quantity</th>\r\n                        <th class=\"text-left\">Total</th>\r\n                        <th class=\"text-left\">Actions</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"(value, index) in detailProduct\" :key=\"index\">\r\n                        <td>\r\n                          <v-img\r\n                            class=\"ma-2 pa-2\"\r\n                            :src=\"value.image_url\"\r\n                            max-width=\"70\"\r\n                            max-height=\"70\"\r\n                          ></v-img>\r\n                        </td>\r\n                        <td>\r\n                          <v-list>\r\n                            <v-list-item-title>\r\n                              <h3>{{ value.product_name }}</h3>\r\n                              <caption v-if=\"value.supplier_name != null\">\r\n                                By\r\n                                {{\r\n                                  value.supplier_name\r\n                                }}\r\n                              </caption>\r\n                            </v-list-item-title>\r\n                            <v-list-item-subtitle v-if=\"value.note != null\">\r\n                              Note : {{ value.note }}\r\n                            </v-list-item-subtitle>\r\n                            <v-list-item-subtitle>\r\n                              <v-btn\r\n                                text\r\n                                x-small\r\n                                @click=\"openNote(index)\"\r\n                                color=\"primary\"\r\n                                >Add Note</v-btn\r\n                              >\r\n                            </v-list-item-subtitle>\r\n                          </v-list>\r\n                        </td>\r\n                        <td>{{ value.price }}</td>\r\n                        <td>\r\n                          <v-text-field\r\n                            v-model=\"quantity\"\r\n                            class=\"editWidth\"\r\n                            type=\"number\"\r\n                            @change=\"calculateTotal(index)\"\r\n                          ></v-text-field>\r\n                        </td>\r\n\r\n                        <td>{{ totalProduct }}</td>\r\n                        <td>\r\n                          <v-icon @click=\"removeProduct\">clear</v-icon>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </template>\r\n                </v-simple-table>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-form>\r\n      </v-col>\r\n      <v-divider class=\"mt-5\"></v-divider>\r\n      <v-card-actions>\r\n        <v-spacer></v-spacer>\r\n        <v-btn color=\"error\" @click=\"cancelData\"> Cancel </v-btn>\r\n        <v-btn color=\"primary\" @click=\"saveData(index)\"> Save </v-btn>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport Proxy from \"@/services/proxy\";\r\nexport default {\r\n  name: \"createOrder\",\r\n  data() {\r\n    return {\r\n      date: \"\",\r\n      menu: false,\r\n      dialog: false,\r\n      dialogNote: false,\r\n      dataCustomer: [],\r\n      typeOrder: [\r\n        { id: 1, name: \"Income\" },\r\n        { id: 2, name: \"Outcome\" },\r\n      ],\r\n      inputData: {\r\n        name_client: \"\",\r\n        type: \"\",\r\n        currency: \"\",\r\n        date: \"\",\r\n      },\r\n      snackbar: {\r\n        show: false,\r\n        text: \"Data Added Successfully\",\r\n        color: \"success\",\r\n      },\r\n      currencyList: [\"USD\", \"IDR\", \"CNY\"],\r\n      detailProduct: [],\r\n      dataProductList: [],\r\n      noteProduct: \"\",\r\n      selectProduct: \"\",\r\n      queryTerm: \"\",\r\n      valueProduct: [],\r\n      activeProduct: \"\",\r\n      quantity: \"\",\r\n      totalProduct: 0,\r\n    };\r\n  },\r\n  watch: {\r\n    search: {\r\n      get() {\r\n        this.queryTerm;\r\n      },\r\n      set(searchInput) {\r\n        if (this.queryTerm !== searchInput) {\r\n          this.queryTerm = searchInput;\r\n          this.loadProduct();\r\n        }\r\n      },\r\n    },\r\n  },\r\n  created() {\r\n    this.loadProduct();\r\n  },\r\n\r\n  methods: {\r\n    changeCurrency() {\r\n      this.loadProduct();\r\n    },\r\n\r\n    loadProduct() {\r\n      const uri = \"https://api-dev.phantasmode.com\";\r\n      // get token from localStorage\r\n      const token = localStorage.getItem(\"access_token\");\r\n      // set the headers\r\n      const headers = {\r\n        Authorization: \"Bearer \" + token,\r\n        Accept: \"application/json\",\r\n      };\r\n\r\n      Proxy.get(\r\n        uri +\r\n          \"/api/products/\" +\r\n          \"?product_name=\" +\r\n          (this.queryTerm || \"\") +\r\n          \"&convertTo=\" +\r\n          this.inputData.currency,\r\n        { headers }\r\n      )\r\n        .then((res) => {\r\n          this.dataProduct = res.data;\r\n          this.queryTerm = res.data.total;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n    addProduct() {\r\n      const incomingDataProduct = this.selectProduct;\r\n      this.selectProduct = {};\r\n      this.valueProduct.push(incomingDataProduct);\r\n    },\r\n    close() {\r\n      this.dialog = false;\r\n      this.dialogNote = false;\r\n      this.selectProduct = null;\r\n    },\r\n    save() {\r\n      this.detailProduct = this.valueProduct;\r\n      console.log(this.detailProduct);\r\n      this.close();\r\n    },\r\n    removeProduct(index) {\r\n      this.valueProduct.splice(index, 1);\r\n    },\r\n    openNote(indexProduct) {\r\n      this.activeProduct = indexProduct;\r\n      this.dialogNote = true;\r\n    },\r\n    saveNote() {\r\n      this.detailProduct[this.activeProduct].note = this.noteProduct;\r\n      this.noteProduct = \"\";\r\n      this.dialogNote = false;\r\n    },\r\n    calculateTotal(index) {\r\n      var total = this.detailProduct[index].price * this.quantity;\r\n      this.totalProduct = total;\r\n      console.log(total);\r\n    },\r\n    cancelData() {\r\n      this.$router.push(\"/Menu/Order/\");\r\n    },\r\n    saveData() {\r\n      const uri = \"https://api-dev.phantasmode.com\";\r\n      // get token from localStorage\r\n      const token = localStorage.getItem(\"access_token\");\r\n      // set the headers\r\n      const headers = {\r\n        Authorization: \"Bearer \" + token,\r\n        Accept: \"application/json\",\r\n      };\r\n      const body = {\r\n        type: this.inputData.type,\r\n        client_name: this.inputData.name_client,\r\n        currency: this.inputData.currency,\r\n        order_date: this.inputData.date,\r\n        items: [\r\n          {\r\n            product_title: this.detailProduct[0].product_name,\r\n            quantity: this.quantity,\r\n            price: this.detailProduct[0].price,\r\n            image_url: this.detailProduct[0].image_url,\r\n            note: this.detailProduct[0].note,\r\n          },\r\n        ],\r\n      };\r\n      Proxy.post(uri + \"/api/orders\", body, { headers })\r\n        .then((res) => {\r\n          console.log(res);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      console.log(body);\r\n      this.snackbar.show = true;\r\n      this.$router.push(\"/Menu/Order\");\r\n    },\r\n  },\r\n  mounted() {\r\n    const uri = \"https://api-dev.phantasmode.com\";\r\n    // get token from localStorage\r\n    const token = localStorage.getItem(\"access_token\");\r\n    // set the headers\r\n    const headers = {\r\n      Authorization: \"Bearer \" + token,\r\n      Accept: \"application/json\",\r\n    };\r\n    Proxy.get(uri + \"/api/contacts\" + \"?type=1\", { headers })\r\n      .then((res) => {\r\n        this.dataCustomer = res.data.data;\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.editWidth {\r\n  width: 50px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/dataOrder"}]}